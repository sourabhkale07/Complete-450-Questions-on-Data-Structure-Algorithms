#include <iostream>

using namespace std;

int main()
{
    int arr[] = {21,23,54,5,69,56};
    int size = sizeof(arr)/sizeof(arr[0]);
    
    int min = arr[0];
    int max = arr[0];
    
   for(int i=0;i<size;i++){
       if(arr[i]<min){
           min = arr[i];
       }
       
    }
   cout<<"Minimum :"<<min<<endl;
   
   for(int i=0;i<size;i++){
       if(arr[i]>max){
           max = arr[i];
       }
    }
   
   cout<<"Maximum :"<<max<<endl;
    
    
    return 0;
}
